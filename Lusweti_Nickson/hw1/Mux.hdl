/** 
 * Multiplexor:
 * if (sel = 0) out = a, else out = b
 */
CHIP Mux {
    IN a, b, sel;
    OUT out;

    PARTS:
    Nand(a=sel , b=b , out=nandBB );
    And(a=a , b=nandBB , out=out );

    //// Replace this comment with your code.
}